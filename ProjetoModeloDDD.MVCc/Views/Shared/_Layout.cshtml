<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>


    <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">

        <a class="navbar-brand" href="#" ml-lg-5>

            ASP.NET
        </a>



        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">

                <li class="nav-item">
                    <a class="nav-link" href="/Home/Index">Home</a>
                </li>

                <li class="nav-item">
                    <a class="nav-link" href="/Clientes/Index">Cliente</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/Produto/Index">Produtos</a>
                </li>

                <li class="nav-item">
                    <a class="nav-link" href="/Venda/Index">Venda</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/Graphics/Index">Graphics</a>
                </li>

            </ul>

        </div>
    </nav>





    <div class="container body-content mt-lg-5">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/fontawesome")
    @Scripts.Render("~/bundles/highcharts")
    @Scripts.Render("~/bundles/bootstrap")

    @RenderSection("scripts", required: false)
</body>
</html>
